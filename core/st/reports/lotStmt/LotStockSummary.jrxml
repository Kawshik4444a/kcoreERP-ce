	<queryString>
		<![CDATA[SELECT a.*, upper(left(a.material_name, 1)) as material_alphabet, case when a.balance_qty_base >= 0 then 0 else 1 end as balance
FROM st.fn_material_balance_wac_report
(	$P{pcompany_id}, 
	$P{pbranch_id}, 
	$P{pmaterial_id},
	$P{pyear}, 
	$P{pto_date}::Date,
	$P{pstock_location_id}
) a
inner join st.material b on a.material_id = b.material_id
Where (a.material_type_id = $P{pmaterial_type_id} Or $P{pmaterial_type_id} = 0)
		And (b.annex_info->'qc_info'->>'has_qc')::Boolean
ORDER BY balance,  a.material_type,	a.material_name;]]>
	</queryString>
