	<queryString>
		<![CDATA[SELECT a.*, upper(left(a.material_name, 1)) as material_alphabet, case when a.balance_qty_base >= 0 then 0 else 1 end as balance
FROM st.fn_material_balance_wac_report
(	$P{pcompany_id}, 
	$P{pbranch_id}, 
	0, 
	$P{pyear}, 
	$P{pto_date}::Date,
	$P{psl_id}) a
Where (material_type_id = $P{pmaterial_type_id} Or $P{pmaterial_type_id} = 0)
ORDER BY balance, 
    case $P{pgroup_type} when 1 then material_type else account_head end ASC,
	material_name;]]>
	</queryString>
