	<queryString>
		<![CDATA[SELECT *, case when cl_bal_qty  >= 0 then 0 else 1 end as balance
FROM st.fn_material_opcl_wac_qty_value_report
(	$P{pcompany_id}, 
	$P{pbranch_id}, 
	$P{pyear}, 
	$P{pfrom_date}::Date,
	$P{pto_date}::Date,
	$P{psuppress_blank},
	$P{psl_id})
Where (material_type_id = $P{pmaterial_type_id} Or $P{pmaterial_type_id} = 0)
ORDER BY balance,  
	case $P{pgroup_type} when 1 then material_type else account_head end ASC,
	material_name;]]>
	</queryString>
