	<queryString>
		<![CDATA[select a.voucher_id, a.doc_date, a.fc_type_id,
  b.account_head, a.exch_rate, a.status, coalesce(a.credit_amt,0) as credit_amt, 
   a.narration, a.amt_in_words, a.amt_in_words_fc, a.remarks, 
   c.entered_by, c.posted_by, c.entered_on  AT TIME ZONE 'UTC' as entered_on, 
   c.posted_on AT TIME ZONE  'UTC' as posted_on,
   d.pay_cycle
from ap.pymt_control a
	inner join ac.account_head b on  (a.annex_info->>'bank_account_id')::bigint = b.account_id
	inner join sys.doc_es c on a.voucher_id = c.voucher_id
    inner join ap.pay_cycle d on (a.annex_info->>'pay_cycle_id')::bigint =d.pay_cycle_id
	where a.voucher_id = $P{pvoucher_id}]]>
	</queryString>
