	<queryString>
		<![CDATA[with mcp_temp
As (
     select $P{pvoucher_id}::varchar voucher_id, sum(a.credit_amt) as credit_amt, sum(a.credit_amt_fc) as credit_amt_fc 
     from ap.fn_pymt_info_for_gl_post($P{pvoucher_id}) a
     inner join ac.account_head b on a.account_id = b.account_id
     where b.account_type_id = 1
)    
select a.voucher_id, a.cheque_date doc_date, a.supplier_detail, a.is_ac_payee, a.is_non_negotiable, b.credit_amt, b.credit_amt_fc
from ap.pymt_control a
Inner join mcp_temp b on a.voucher_id = b.voucher_id
where a.voucher_id = $P{pvoucher_id}]]>
	</queryString>
