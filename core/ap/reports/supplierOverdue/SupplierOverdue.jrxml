	<queryString>
		<![CDATA[Select  a.con_account_id, a.con_account_head, a.account_id, a.account_head, sum(a.overdue) as overdue, sum(a.overdue_fc) as overdue_fc, sum(a.not_due) as not_due, sum(a.not_due_fc) as not_due_fc,
				sum(a.adv_amt) as adv_amt, sum(a.adv_amt_fc) as adv_amt_fc
from ap.fn_supplier_overdue_with_adv(
	$P{pcompany_id}, 
	$P{pbranch_id}, 
	$P{psupplier_id}, 
	$P{pto_date}::Date, 
	$P{pvoucher_id},
	$P{ppdc},
	$P{pcon_account_id},
	$P{pcurr_pos},
   $P{preg_msmeda}
)  a	
group by  a.con_account_id, a.con_account_head, a.account_id, a.account_head
order by  a.con_account_head, a.account_head;]]>
