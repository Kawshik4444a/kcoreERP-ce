	<queryString>
		<![CDATA[With gstr2a as
(select z.voucher_id from tx.gstr2a z group by z.voucher_id) 
select 'Assets' account_group, COALESCE(sum(bt_amt),0) bt_amt, COALESCE(sum(sgst_amt),0) sgst_amt, COALESCE(sum(cgst_amt),0) cgst_amt, COALESCE(sum(igst_amt),0) igst_amt
from (select x.* from ap.fn_purchase_register_report( 
	$P{pcompany_id}, 
	$P{pbranch_id}, 
	$P{psupplier_id}, 
	$P{pfrom_date}::Date, 
	$P{pto_date}::Date, 
	$P{pgst_state_id},
	'A001',
	$P{pinclude_non_gst}
) x
left join gstr2a y on x.voucher_id=y.voucher_id
where   char_length(x.gstin) > 2  and y.voucher_id is null ) z
Union All
select 'Cost of Goods Consumed', COALESCE(sum(bt_amt),0) bt_amt, COALESCE(sum(sgst_amt),0) sgst_amt, COALESCE(sum(cgst_amt),0) cgst_amt, COALESCE(sum(igst_amt),0) igst_amt
from (select x.* from ap.fn_purchase_register_report( 
	$P{pcompany_id}, 
	$P{pbranch_id}, 
	$P{psupplier_id}, 
	$P{pfrom_date}::Date, 
	$P{pto_date}::Date, 
	$P{pgst_state_id},
	'A005',
	$P{pinclude_non_gst}
) x
left join gstr2a y on x.voucher_id=y.voucher_id
where   char_length(x.gstin) > 2  and  y.voucher_id is null ) z
Union All
select 'Expenses', COALESCE(sum(bt_amt),0) bt_amt, COALESCE(sum(sgst_amt),0) sgst_amt, COALESCE(sum(cgst_amt),0) cgst_amt, COALESCE(sum(igst_amt),0) igst_amt
from (select x.* from ap.fn_purchase_register_report( 
	$P{pcompany_id}, 
	$P{pbranch_id}, 
	$P{psupplier_id}, 
	$P{pfrom_date}::Date, 
	$P{pto_date}::Date, 
	$P{pgst_state_id},
	'A006',
	$P{pinclude_non_gst}
) x
left join gstr2a y on x.voucher_id=y.voucher_id
where  char_length(x.gstin) > 2  and   y.voucher_id is null) z
Union All
select 'Non Expense', COALESCE(sum(bt_amt),0) bt_amt, COALESCE(sum(sgst_amt),0) sgst_amt, COALESCE(sum(cgst_amt),0) cgst_amt, COALESCE(sum(igst_amt),0) igst_amt
from (select x.* from ap.fn_purchase_register_report( 
	$P{pcompany_id}, 
	$P{pbranch_id}, 
	$P{psupplier_id}, 
	$P{pfrom_date}::Date, 
	$P{pto_date}::Date, 
	$P{pgst_state_id},
	'NX',
	$P{pinclude_non_gst}
) x
left join gstr2a y on x.voucher_id=y.voucher_id
where  char_length(x.gstin) > 2  and  y.voucher_id is null) z
]]>
	</queryString>
