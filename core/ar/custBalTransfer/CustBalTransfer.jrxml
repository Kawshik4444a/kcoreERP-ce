	<queryString>
		<![CDATA[select 	a.voucher_id, a.branch_id, a.doc_date, a.fc_type_id, e.fc_type, a.exch_rate, a.status, a.received_from,
        a.account_id, a.customer_account_id, c.account_head as customer_account_head, a.narration, a.amt_in_words, a.amt_in_words_fc,
        a.remarks, a.debit_amt, a.debit_amt_fc, d.entered_by, d.posted_by, a.net_settled, a.net_settled_fc,
        d.entered_on  AT TIME ZONE 'UTC' as entered_on, 
	    d.posted_on AT TIME ZONE  'UTC' as posted_on
from ar.rcpt_control a
inner join ac.account_head c on a.customer_account_id = c.account_id
inner join sys.doc_es d on a.voucher_id = d.voucher_id
inner join ac.fc_type e on a.fc_type_id = e.fc_type_id
where a.voucher_id = $P{pvoucher_id};]]>
