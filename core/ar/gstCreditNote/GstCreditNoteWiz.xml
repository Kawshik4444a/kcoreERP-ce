        <className>\app\core\ar\gstCreditNote\GstCreditNoteWiz</className>
    </codeBehind>
    <wizardStep id="SelectCustomer">
        <path>gstCreditNote/GstCreditNoteWiz/SelectCustomer</path>
    </wizardStep>
