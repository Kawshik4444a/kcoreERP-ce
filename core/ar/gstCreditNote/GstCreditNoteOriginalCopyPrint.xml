<cwFramework xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
             xmlns='http://xml.coreerp.com/schema/CwFramework'
             xsi:schemaLocation='http://xml.coreerp.com/schema/CwFramework http://coreerp.com/cwFramework.xsd'>

    <reportView id="GstCreditNote" type="Report" rptPath="/core/ar/gstCreditNote" rptName="GstCreditNote">
        <header>Credit Note</header>
        <controlSection editMode="Edit">
            <dataBinding>
                <field id="pvoucher_id" label="Voucher No" type="string" control="TextBox" size="l"/>            
                <param id="pcompany_id">
                    <session>company_id</session>
                </param>
                <param id="pbranch_id">
                    <session>branch_id</session>
                </param>  
                <param id="pcp_option">
                    <text>3</text>
                </param>
                <rptCompanyInfoPrefix>inv_</rptCompanyInfoPrefix>
            </dataBinding>
        </controlSection>  
    </reportView>
</cwFramework>