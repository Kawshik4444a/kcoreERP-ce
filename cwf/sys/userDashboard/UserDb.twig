    <script id="tmpl-apr" type="text/html">
        <tr> 
            <td data-bind="text: bo_id">
            </td>
            <td data-bind="text: doc_id">
            </td>
            <td data-bind="dateValue: doc_date, attr:{'data-sort': doc_date_sort}">
            </td>                
            <td data-bind="text: from_user">
            </td>
            <td data-bind="dateValue: added_on, attr:{'data-sort': req_date_sort}">
            </td> 
            <td>
                <button type="button" id="cedit" title="View" 
                        data-bind ="click:coreWebApp.wf_userdocs.openSummary"
                        style="border:none;padding-left:5px;padding-right:5px;background-color:white;">
                    <i class="glyphicon glyphicon-info-sign"></i>
                </button>
            </td>                
        </tr>
    </script>
    <script id="tmpl-apr1" type="text/html">
        <div class="row col-md-12" style="margin-top: 10px;margin-left: 10px;text-align: center;padding: 0;"> 
            <div class="col-md-2" data-bind="text: bo_id">
            </div>
            <div class="col-md-2" data-bind="text: doc_id">
            </div>
            <div class="col-md-2" data-bind="dateValue: doc_date, attr:{'data-sort': doc_date_sort}">
            </div>                
            <div class="col-md-2" data-bind="text: from_user">
            </div>
            <div class="col-md-2" data-bind="dateValue: added_on, attr:{'data-sort': req_date_sort}">
            </div> 
            <div class="col-md-2">
                <button type="button" id="cedit" title="View" 
                        data-bind ="click:user_db.udocs.openSummary"
                        style="border:none;padding-left:5px;padding-right:5px;background-color:white;">
                    <i class="glyphicon glyphicon-info-sign"></i>
                </button>
            </div>                
        </div>
    </script>

    <script id="tmpl-doc" type="text/html">
        <tr> 
            <td data-bind="text: branch_name">
            </td>
            <td data-bind="text: bo_id">
            </td>
            <td data-bind="text: doc_id">
            </td>
            <td data-bind="dateValue: doc_sent_on, attr:{'data-sort': doc_date_sort}">
            </td>
            <td>
                <select id="wf_id" class="form-control" name="wf_id" data-bind="value: doc_action" disabled="true">
                    <option value="S">Sent</option>
                    <option value="A">Approved</option>
                    <option value="R">Rejected</option>
                    <option value="P">Posted</option>
                    <option value="U">Unposted</option>
                    <option value="I">Assigned</option>
                </select>
            </td>
            <td data-bind="text: from_user">
            </td>                
        </tr>
    </script>
    <script id="tmpl-doc1" type="text/html">
        <div class="row col-md-12" style="margin-top: 10px;margin-left: 10px;text-align: center;padding: 0;"> 
            <div class="col-md-2" data-bind="text: branch_name">
            </div>
            <div class="col-md-2" data-bind="text: bo_id">
            </div>
            <div class="col-md-2" data-bind="text: doc_id">
            </div>
            <div class="col-md-2" data-bind="dateValue: doc_sent_on, attr:{'data-sort': doc_date_sort}">
            </div>
            <div class="col-md-2">
                <select id="wf_id" class="form-control" name="wf_id" data-bind="value: doc_action" disabled="true">
                    <option value="S">Sent</option>
                    <option value="A">Approved</option>
                    <option value="R">Rejected</option>
                    <option value="P">Posted</option>
                    <option value="U">Unposted</option>
                    <option value="I">Assigned</option>
                </select>
            </div>
            <div class="col-md-2" data-bind="text: from_user">
            </div>                
        </div>
    </script>

    <script src="{{ ScriptHelper.registerScript('@app/cwf/sys/userDashboard/userDb.js') }}"></script>
    <script type="text/javascript">
    window.user_db.udocs.userdb_pageload();
    $('body').children('#tooltip').remove();
    $("<div id='tooltip'></div>").css({
        position: "absolute",
        display: "none",
        border: "1px solid #fdd",
        padding: "2px",
        "background-color": "#fee",
        opacity: 0.80,
        "line-height": 1.2
    }).appendTo("body");
    </script>