             xsi:schemaLocation='http://xml.coreerp.com/schema/CwFramework http://coreerp.com/cwFramework.xsd'>

    <lookup id="Priority">
        <connectionType>
            <mainDB/>
        </connectionType>
        <sql>
            <command>
                Select priority_id,  description from sys.feedback_priority
